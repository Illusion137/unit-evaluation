            { "name": "v", "unit": [1, -1, 0, 0, 0, 0, 0], "description": "Fluid velocity", "constant": false }
        ]
    },
    {
        "name": "Mass Flow Rate",
        "latex": "\\dot{m} = \\rho A v",
        "variables": [
