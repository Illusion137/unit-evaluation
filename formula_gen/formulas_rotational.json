        ]
    },
    {
        "name": "Tangential Velocity",
        "latex": "v = r\\omega",
        "variables": [
            { "name": "v", "unit": [1, -1, 0, 0, 0, 0, 0], "description": "Tangential velocity", "constant": false },
            { "name": "r", "unit": [1, 0, 0, 0, 0, 0, 0], "description": "Radius", "constant": false },
            { "name": "\\omega", "unit": [0, -1, 0, 0, 0, 0, 0], "description": "Angular velocity", "constant": false }
        ]
    },
    {
        "name": "Tangential Acceleration",
        "latex": "a_t = r\\alpha",
        "variables": [
            { "name": "a_t", "unit": [1, -2, 0, 0, 0, 0, 0], "description": "Tangential acceleration", "constant": false },
            { "name": "r", "unit": [1, 0, 0, 0, 0, 0, 0], "description": "Radius", "constant": false },
            { "name": "\\alpha", "unit": [0, -2, 0, 0, 0, 0, 0], "description": "Angular acceleration", "constant": false }
        ]
    }
]
