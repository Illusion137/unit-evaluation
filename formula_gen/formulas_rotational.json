        ]
    },
    {
        "name": "Rotational Displacement with Acceleration",
        "latex": "\\theta = \\omega_0 t + \\frac{1}{2}\\alpha t^2",
        "variables": [
            { "name": "\\theta", "unit": [0, 0, 0, 0, 0, 0, 0], "description": "Angular displacement", "constant": false },
            { "name": "\\omega_0", "unit": [0, -1, 0, 0, 0, 0, 0], "description": "Initial angular velocity", "constant": false },
            { "name": "t", "unit": [0, 1, 0, 0, 0, 0, 0], "description": "Time", "constant": false },
            { "name": "\\alpha", "unit": [0, -2, 0, 0, 0, 0, 0], "description": "Angular acceleration", "constant": false }
        ]
    },
    {
        "name": "Rotational Kinetic Energy",
        "latex": "KE_{rot} = \\frac{1}{2}I\\omega^2",
        "variables": [
            { "name": "KE_rot", "unit": [2, -2, 1, 0, 0, 0, 0], "description": "Rotational kinetic energy", "constant": false },
            { "name": "I", "unit": [2, 0, 1, 0, 0, 0, 0], "description": "Moment of inertia", "constant": false },
            { "name": "\\omega", "unit": [0, -1, 0, 0, 0, 0, 0], "description": "Angular velocity", "constant": false }
        ]
    },
    {
        "name": "Angular Momentum",
        "latex": "L = I\\omega",
        "variables": [
            { "name": "L", "unit": [2, -1, 1, 0, 0, 0, 0], "description": "Angular momentum", "constant": false },
            { "name": "I", "unit": [2, 0, 1, 0, 0, 0, 0], "description": "Moment of inertia", "constant": false },
            { "name": "\\omega", "unit": [0, -1, 0, 0, 0, 0, 0], "description": "Angular velocity", "constant": false }
        ]
    },
    {
        "name": "Moment of Inertia (Point Mass)",
        "latex": "I = mr^2",
        "variables": [
            { "name": "I", "unit": [2, 0, 1, 0, 0, 0, 0], "description": "Moment of inertia", "constant": false },
