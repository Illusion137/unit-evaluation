[
    {
        "name": "Torque",
        "latex": "\\tau = rF",
        "variables": [
            { "name": "\\tau", "unit": [2, -2, 1, 0, 0, 0, 0], "description": "Torque", "constant": false },
            { "name": "r", "unit": [1, 0, 0, 0, 0, 0, 0], "description": "Moment arm (radius)", "constant": false },
