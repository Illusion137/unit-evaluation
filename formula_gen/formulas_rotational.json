        ]
    },
    {
        "name": "Torque from Moment of Inertia",
        "latex": "\\tau = I\\alpha",
        "variables": [
            { "name": "\\tau", "unit": [2, -2, 1, 0, 0, 0, 0], "description": "Torque", "constant": false },
            { "name": "I", "unit": [2, 0, 1, 0, 0, 0, 0], "description": "Moment of inertia", "constant": false },
            { "name": "\\alpha", "unit": [0, -2, 0, 0, 0, 0, 0], "description": "Angular acceleration", "constant": false }
        ]
    },
    {
        "name": "Angular Velocity",
        "latex": "\\omega = \\frac{\\theta}{t}",
        "variables": [
            { "name": "\\omega", "unit": [0, -1, 0, 0, 0, 0, 0], "description": "Angular velocity", "constant": false },
            { "name": "\\theta", "unit": [0, 0, 0, 0, 0, 0, 0], "description": "Angle", "constant": false },
            { "name": "t", "unit": [0, 1, 0, 0, 0, 0, 0], "description": "Time", "constant": false }
        ]
    },
    {
        "name": "Angular Acceleration",
        "latex": "\\alpha = \\frac{\\omega - \\omega_0}{t}",
        "variables": [
            { "name": "\\alpha", "unit": [0, -2, 0, 0, 0, 0, 0], "description": "Angular acceleration", "constant": false },
            { "name": "\\omega", "unit": [0, -1, 0, 0, 0, 0, 0], "description": "Final angular velocity", "constant": false },
            { "name": "\\omega_0", "unit": [0, -1, 0, 0, 0, 0, 0], "description": "Initial angular velocity", "constant": false },
            { "name": "t", "unit": [0, 1, 0, 0, 0, 0, 0], "description": "Time", "constant": false }
        ]
    },
    {
        "name": "Rotational Velocity with Acceleration",
        "latex": "\\omega = \\omega_0 + \\alpha t",
        "variables": [
            { "name": "\\omega", "unit": [0, -1, 0, 0, 0, 0, 0], "description": "Final angular velocity", "constant": false },
            { "name": "\\omega_0", "unit": [0, -1, 0, 0, 0, 0, 0], "description": "Initial angular velocity", "constant": false },
            { "name": "\\alpha", "unit": [0, -2, 0, 0, 0, 0, 0], "description": "Angular acceleration", "constant": false },
